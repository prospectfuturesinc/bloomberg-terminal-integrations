# Bloomberg Terminal API Configuration
BLOOMBERG_API_KEY=your_api_key_here
BLOOMBERG_API_SECRET=your_api_secret_here
BLOOMBERG_SERVER_HOST=api.bloomberg.com
BLOOMBERG_SERVER_PORT=8194

# Connection Settings
BLOOMBERG_TIMEOUT=30000
BLOOMBERG_ENABLE_WEBSOCKET=true

# Excel Export Settings
EXCEL_AUTO_REFRESH=false
EXCEL_REFRESH_INTERVAL=5000
EXCEL_ENABLE_FORMATTING=true
EXCEL_INCLUDE_HEADERS=true

# Logging
LOG_LEVEL=info
NODE_ENV=development

# Output Paths
EXCEL_OUTPUT_PATH=./output
